generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Skill {
  id        Int      @id @default(autoincrement())
  name      String   @unique
  createdAt DateTime @default(now())
}

model Project {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  tech        String
  github      String
  createdAt   DateTime @default(now())
}
